generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Donation {
  id            Int      @id @default(autoincrement())
  sessionId     String   @unique
  amount        Int
  email         String?
  name          String?
  status        String
  createdAt     DateTime @default(now())
}

